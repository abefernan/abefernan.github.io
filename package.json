{
  "name": "abefernan-site",
  "version": "1.0.0",
  "author": "Abel Fern√°ndez <abel.fernandez.nandin@gmail.com>",
  "description": "abefernan's personal site",
  "license": "Apache-2.0",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/react": "^10.0.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-parcel": "^1.10.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "parcel": "^2.0.0-nightly.277",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "dev": "parcel serve src/index.html --open",
    "prebuild": "rm -f dist/*",
    "build": "parcel build src/index.html",
    "postbuild": "mv dist/index.html index.html && sed -i 's/\\/abefernan/.\\/dist\\/abefernan/g' index.html",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  }
}
